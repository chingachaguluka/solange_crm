
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header"><%= "#{@contact.first_name}  #{@contact.last_name}" %></h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-9 col-md-9">
            <div class="panel panel-primary">
              <!-- /.panel-heading -->

              <div class="panel-body">
              	<div class="table-responsive">
                  <table class="table table-striped table-bordered table-hover">
                    <tbody>
	                    <tr>
	                    	<td>Title:</td>
	                    	<td><%= @contact.title %></td>
	                    </tr>
	                    <tr>
	                    	<td>First Name:</td>
	                    	<td><%= @contact.first_name %></td>
	                    </tr>
	                    <tr>
	                    	<td>Last Name:</td>
	                    	<td><%= @contact.last_name %></td>
	                    </tr>
	                    <tr>
	                    	<td>Cell:</td>
	                    	<td><%= @contact.cell_phone %></td>
	                    </tr>
	                    <tr>
	                    	<td>Email:</td>
	                    	<td><%= @contact.email %></td>
	                    </tr>
	                     <tr>
	                    	<td>Parent Account:</td>
                        <% if @contact.account_id.present? %>
	                    	    <td><%= @contact.account.name %></td> %>
                        <% else %>
                            <td> Not Assigned </td> %>
                        <% end %>
	                    </tr>
                    </tbody>
                  </table>
	              </div>
	              <!-- /.table-responsive -->

	              
              </div>
              <!-- /.panel-body -->        
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-9 -->
        <div class="col-lg-3 col-md-3">
        	<div class="panel panel-primary">
              <!-- /.panel-heading -->

              <div class="panel-body">
              	<p><%= link_to "All Contacts", contacts_path %></p>
              	<p><%= link_to "Edit Contact", edit_contact_path(@contact) %></p>
              	<p><%= link_to "Delete Contact", @contact, method: :delete, data: { confirm: "You sure?" } %></p>
                <p><%= link_to "New Contact", new_contact_path %></p>
              </div>
              <!-- /.panel-body -->        
            </div>
            <!-- /.panel -->
        </div>
        <!-- end div col-lg-3 -->
    </div>
    <!-- /.row -->
    </div>
    <!-- /#page-wrapper -->

</div>